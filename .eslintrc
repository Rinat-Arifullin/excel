{
    "parser": "babel-eslint",
    "env":{
        "es6" : true,
        "browser" : true,
        "node" : true
    },
    "rules":{
        "semi": "off",
        "expected_a_b": "off",
        "es6": "off",
        "linebreak-style": ["error", "windows"],
        "require-jsdoc" : 0,
        "operator-linebreak": "off",
        "indent": "off"
    },
    "extends": ["eslint:recommended", "google"]
}